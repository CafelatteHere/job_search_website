    <div class="contents">
      <%= form_with(url: search_vacancies_path, local: true, method: :get, class: "search-form") do |form| %>
        <%= form.text_field :keyword, placeholder: "応募を検索する", class: "search-input" %>
        <%= form.submit "検索", class: "search-btn"%>
      <% end %>
      
      <div class="vacancies">
        <% @vacancies.each do |vacancy| %>
          <%= render partial: 'vacancy' , locals: {vacancy: vacancy}  %>
        <% end %>
      </div>
    </div>
